
#
# Near Field Communication (NFC) devices
#

config HUAWEI_HW_DEV_DCT
	depends on HUAWEI_KERNEL
	default n
	bool "HUAWEI DEVICE DETECT"
	help
		Say yes if you want i2c device detect driver.
		This is for i2c connected version. If unsure, say N here.

config HUAWEI_HW_SIM_DETECT
	depends on HUAWEI_KERNEL
	default n
	bool "HUAWEI SIM DEVICE DETECT"
	help
		Say yes if you want sim device detect driver.
		This is for sim plugin version. If unsure, say N here.
